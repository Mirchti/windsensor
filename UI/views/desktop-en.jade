								div(id='graph-aver-speed-month-axis1').x_axis
								p(id='graph-aver-speed-month-label1').x_label
						h3 windspeed over 4 m/s
						div.well
							div(id='graph-over-4msec-month' name='graph').graph-container
								p(id='graph-over-4msec-month-label0').y_label
								div(id='graph-over-4msec-month-axis0').y_axis
								div(id='graph-over-4msec-month-chart').chart
								div(id='graph-over-4msec-month-legend').legend
