								div(id='graph-electricpowerval-month-axis1').x_axis
								p(id='graph-electricpowerval-month-label1').x_label
						h3 actual values
						div.well
							div
								p(style='display:inline') maximum windspeed:
								p(id='top-speed-month' style='display:inline')
							div
								p(style='display:inline') average windspeed:
								p(id='aver-speed-month' style='display:inline')
							div
