							div
								p(style='display:inline') electrical work:
								p(id='electricpowerval-month' style='display:inline')
					div(id='tabdays').tab-pane
						div.collapse.navbar-collapse
							ul.nav.navbar-nav
								li.dropdown
									a(href='#' data-toggle='dropdown' class='dropdown-toggle') change day
										b(class='caret') 
									ul(id='dd_days').dropdown-menu
										each d in days
											li(id='#{d}' name='day').dpmenu #{d}
						h1(style='display:inline') day
						h1(id='title-day' style='display:inline')
						h3 histogramm
						div.well
							div(id='graph-hist-day').graph-container.multi
								p(id='graph-hist-day-label0').y_label
								div(id='graph-hist-day-axis0').y_axis
